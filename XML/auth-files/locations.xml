<!--whc: there are two listPlace elements below, and two lists, since there are different TEI formats for different types of places.
            The first listPlace is for human geography only: settlements, regions, countries, etc. Numbering begins with 1.
            The second listPlace, further down, is for natural geography only: rivers, mountains, and other physical features.-->
         <!--whc: for map, see https://www.google.com/maps/d/edit?mid=1OOVy7lKC6eSBamlDAqrFRAdkHN8VAyg  -->


         <listPlace type="human-geography">
            
            <place n="1">
               <placeName><settlement type="region">Alcántara</settlement></placeName>
               <country>Chile</country>
               <region>Maule Region</region>
               <location><geo>-34.84532, -71.9831</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">Region near Lake Tilicura</note>
            </place>
            
            <place n="2">
               <placeName><settlement type="city">Angol</settlement></placeName>
               <country>Chile</country>
               <region>Malleco Region</region>
               <location><geo>-37.79995460880765, -72.70533700360946</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">capital city of the Malleco Region in Araucanía</note>
            </place>
            
            <place n="3">
               <placeName><settlement type="city">Arauco</settlement></placeName>
               <country>Chile</country>
               <region>Bío Bío Region</region>
               <location><geo></geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">a settlement on the southern shore of the Bay of Arauco, often fought over and repeatedly razed and rebuilt between the Spanish Conquest and the War of Independence</note>
            </place>
            
            <place n="4">
               <placeName><settlement type="village">Boroa de Imperial (Boca de Ymperial)</settlement></placeName>
               <country>Chile</country>
               <region>Araucanía Region</region>
               <location><geo>-38.766196246685766, -72.85164657908746</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">town on the north bank of the Cautín River in Araucanía</note>
            </place>
            
            <place n="5">
               <placeName><settlement type="province">Buenos Aires (Ayres)</settlement></placeName>
               <country>Argentina</country>
               <region>Province of Buenos Aires</region>
               <location><geo>-33.7, -61</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">In the Treaty of Negrete (1803), there are several references to the Pampas of Buenos Aires, which lie on the 
               eastern side of the Andes Cordillera</note>
            </place>
            
            <place n="6">
               <placeName><settlement type="city">Chillán</settlement></placeName>
               <country>Chile</country>
               <region>Ñuble Region</region>
               <location><geo>-36.6, -72.116667</geo></location>
               <note xml:lang="span"></note>
               <note xml:lang="eng">city in central Chile on the south bank of the Ñuble River; site of a Franciscan missionary school</note>
            </place>
            
            <place n="7">
               <placeName><settlement type="city">Concepción</settlement></placeName>
               <country>Chile</country>
               <region>Bío Bío Province</region>
               <location><geo>-36.82013, -73.04439</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">a significant city in central Chile, located at the mouth of the Bío Bío River; seat of the second-oldest Catholic diocese in Chile (1563)</note>
            </place>
            
            <place n="8">
               <placeName><settlement type="village">Negrete</settlement></placeName>
               <country>Chile</country>
               <region>Bío Bío Region</region>
               <location><geo>-37.58625, -72.53205</geo></location>
               <note xml:lang="span">Spanish description of Negrete here</note>
               <note xml:lang="eng">a village on the south bank of the Bío Bío River; site of four Parlamentos meetings, in 1726, 1771, 1793, and 1803; also site of a Spanish fort</note>
            </place>
            
            <place n="9">
               <placeName><settlement type="town">Osorno</settlement></placeName>
               <country>Chile</country>
               <region>Osorno Province</region>
               <location><geo>-40.57602579011431, -73.1421079145476</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">town on the Rahue River in south-central Chile; also the site of a Spanish fort</note>
            </place>
            
            <place n="10">
               <placeName><settlement type="village">Quilaco</settlement></placeName>
               <country>Chile</country>
               <region>Bío Bío Province</region>
               <location><geo>-37.68296946147364, -72.00171207342952</geo></location>
               <note xml:lang="span">Spanish description here</note>
               <note xml:lang="eng">a village on the south shore of the Bío Bío River, nearly opposite from Santa Bárbara, 50km/30mi east of Negrete</note>
            </place>
            
            <place n="11">
               <placeName><settlement type="city">Santa Bárbara</settlement></placeName>
               <country>Chile</country>
               <region>Bío Bío Province</region>
